package Excepciones;

/**
 * @author 47767573
 *
 */
public class Ejercicio4 extends ExcepcionPadre{

	String excepcion;
	
	
	/**
	 * @param args
	 */
	public Ejercicio4 (String X){
		super();
		excepcion = X;		
	}
	
	@Override
	public String toString() {
		return excepcion +"es una Excepcion Ejercicio4 de "+ super.toString();
	}
	
		
	
	
}
