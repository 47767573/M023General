
package Excepciones;

/**
 * @author 47767573
 *
 */
class Ejercicio4 extends Exception{

	String excepcion;
	
	
	/**
	 * @param args
	 */
	public Ejercicio4 (String X){
		excepcion = X;		
	}	
	
	public class ClaseExcepcion {
	
		public void main (String[] Args){
			try{
				System.out.println("Esto es una excepcion del ejercicio4");
										
			}catch (Exception e){
				System.out.println("Se produjo excepcion: "+ e.getMessage());
			}finally{
				System.out.println("Ejecutaremos igualmente aunque hayan errores");
			}
		
		}
	
	}
	public String getMensaje() {
		return excepcion;
	}
	
		
	
		
	
	
}
