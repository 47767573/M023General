 */
public class Ejercicio4 extends Exception{

	String excepcion;
	
	
	/**
	 * @param args
	 */
	public Ejercicio4 (String X){
		super();
		excepcion = X;		
	}	
	
	
	public void main (String[] Args){
		try{
			
						
		}catch (Exception e){
			System.out.println("error ejercicio4."+ super.toString());
		}
	
	}
	
	
	@Override
	public String toString() {
		return excepcion +"es una Excepcion Ejercicio4 de "+ super.toString();
	}
	
		
	
	
}
